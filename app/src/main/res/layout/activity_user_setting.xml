<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/setting_person"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_all"
        android:layout_weight="1"
        android:clickable="true"
        android:text="  个人资料"
        android:background="@drawable/bg_btn4_white"
        android:textColor="#5E5E5E"
        android:textSize="15sp"
        android:paddingTop="8dp"/>

    <TextView
        android:id="@+id/setting_all"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_weight="1"
        android:background="@drawable/bg_tab_menu"
        android:text="← 设置"
        android:textSize="20dp"
        android:paddingTop="10sp"
        android:paddingLeft="9dp"



        />

    <TextView
        android:id="@+id/setting_privacy"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="106dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="   隐私设置"
        android:clickable="true"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_renzheng"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:clickable="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="  实名认证"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_binging"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:clickable="true"
        android:layout_marginTop="220dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="  账号管理和绑定"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_store"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:clickable="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="187dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="  缓存管理"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_invist_imate"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:layout_marginBottom="125dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="  邀请好友"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_update"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:layout_marginBottom="64dp"
        android:background="@drawable/bg_btn4_white"
        android:paddingTop="8dp"
        android:text="  检查更新"
        android:textColor="#5E5E5E"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/setting_blank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_person"
        android:background="@drawable/blank_color" />

    <TextView
        android:id="@+id/setting_blank3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_binging"
        android:background="@drawable/blank_color" />

    <TextView
        android:id="@+id/setting_blank4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_store"
        android:layout_marginTop="-187dp"
        android:background="@drawable/blank_color" />

    <TextView
        android:id="@+id/setting_blank5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_invist_imate"
        android:layout_marginTop="-125dp"
        android:background="@drawable/blank_color" />

    <TextView
        android:id="@+id/setting_blank2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/setting_renzheng"
        android:background="@drawable/blank_color" />

    <TextView
        android:id="@+id/setting_blank1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/setting_renzheng"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="-160dp"
        android:background="@drawable/blank_color" />

</RelativeLayout>